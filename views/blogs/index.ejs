<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
  <body>
    <%- include('../partials/nav') %>
    <div class="container">
      <% if(blogs.length > 0) { %> <% blogs.forEach(blog => { %>
      <a href="/blogs/<%= blog._id %>">
        <h3><%= blog.title %></h3>
        <p><%= blog.snippet %></p>
      </a>
      <% }) %> <% } %>
    </div>
  </body>
</html>
